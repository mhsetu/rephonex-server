{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [{ "source": "/(.*)", "destination": "/" }],
  "routes": [
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/other-404.html",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
    }
  ]
}
